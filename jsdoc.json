{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": "./src"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "readme": "README.md",
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}